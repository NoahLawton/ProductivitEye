{
    "manifest_version": 3,
    "name": "ProductivitEye",
    "description": "Base Level Extension",
    "permissions": ["storage"],
    "version": "1.0",
    "action": {
      "default_popup": "settings.html",
      "default_icon": "hello_extensions.png"
    },
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["libs/webgazer.js", "content.js"]
        }
      ],
      "web_accessible_resources": [
        {
          "resources": ["libs/webgazer.js"],
          "matches": ["<all_urls>"]
        }
      ],
      "options_page": "settings.html"
  }